    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "View xlsx",
            "type": "debugpy",
            "request": "launch",
            "program": "custom/view_xlsx_file.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "outputs/Qwen2-VL-2B-Instruct/Qwen2-VL-2B-Instruct_MMStar.xlsx",
                "--lines",
                "0-10"
            ]
        },
        {
            "name": "Eval",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--data",
                "MMStar",
                "BLINK",
                "MMBench_DEV_EN",
                "RealWorldQA",
                "HallusionBench",
                "POPE",
                "MME",
                "MMStar",
                "Blink",
                "HICO",
                "--model",
                "Qwen2-VL-2B-Instruct",
                "Qwen2.5-VL-3B",
                "InternVL2_5-2B",
                "--verbose",
                "--mode",
                "all"
            ]
        },
        {
            "name": "Eval video",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--data",
                "TempCompass_MCQ_8frame",
                "Virat_MCQ",
                "--model",
                "Qwen2.5-VL-7B",
                "Qwen2.5-VL-3B",
                "--verbose",
                "--mode",
                "all",
                "--reuse"
            ]
        },
        {
            "name": "Eval - LocalServer",
            "type": "debugpy",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--data",
                "POPE",
                "--model",
                "LocalServer",
                "--verbose",
                "--mode",
                "all"
            ]
        }
    ]
}